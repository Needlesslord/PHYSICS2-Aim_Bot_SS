<!-- Config file for the game -->

<config>
  <app>
    <title>AimBot</title>
  </app>
	
  <renderer>
    <vsync value="false"/>
  </renderer>

  <audio>
    <volume value="64"></volume>
  </audio>

  <window>
    <resolution width="1024" height="768" scale="1"/>
    <fullscreen value="false"/>
    <borderless value="false"/>
    <resizable value="false"/>
    <fullscreen_window value="false"/>
  </window>

  <scene>
    <bullet>
      <position x="1" y="0"/>
      <elastic value="false"/>
      <edge length="0.1"/>
    </bullet>

    <target>
      <position x="10" y="60"/>
      <edge length="1"/>
    </target>
  </scene>
</config>